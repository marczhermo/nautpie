{
    "chmod": "0755",
    "directories": [
        "src"
    ],
    "files": [
        "LICENSE.txt"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "tests",
                "Tests",
                "phpunit",
                "phpspec",
                "sebastian",
                "phpdocumentor",
                "webmozart",
                ".md",
                ".gitignore"
            ],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "nautpie.php",
    "output": "nautpie.phar",
    "stub": true
}
